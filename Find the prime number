#include<stdio.h>

	

int main()
{
	int number,count=0,div;
	printf("Enter the number you want: \n");
	scanf("%d",&number);
	if(number>1)
	{
		for(int i=2;i<=number;i++)
		{
			
			if(number%i==0)
			{
				count++;
			}
		}
	}
	else
	{
		printf("You entered invalid number");
	}
	
	if(count>1)
	{
		printf("%d  is not prime number",number);
	}
	else
	{
		printf("%d is a prime number",number);
	}
	return 0;
}
